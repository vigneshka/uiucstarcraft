<div id="members">
	
	<% cache("officers") do %>
	<div id="officers" class="box">
		<h1>Officers</h1>
			<h2>President: <br>Jeff Stanislaw</h2>
				<h3>milese1029@gmail.com</h3>
				<p>I organize cstarleague competitions and other events.</p>
			<h2>Webmaster: <br>Vignesh Hirudayakanth</h2>
				<h3>vigneshkanth@gmail.com</h3>
				<p>I created and maintain this website.</p>

	</div>
	<% end %>

	<div id="ranking">
		<p>Click the player name for full statistics.
			If you would like to be included in this listing,
			please send your <a href="http://www.sc2ranks.com/">sc2ranks</a>
			profile via the <a href="/contact">contact form</a>.</p>
		<% cache("table", :expires_in => 1.hour) do %>
		<table>
			<% @data.each do |player| %>
			<tr>
				<td class="<%="img-#{player[0]} race" %>"></td> 
				<td><%= link_to("#{player[1]}", "http://sc2ranks.com/us/#{player[4]}/#{player[1]}")%></td>
				<td class="<%= "img-#{player[2]} league"%>"> </td> 
				<td class="points"><%=	player[3] %></td>
			</tr>
			<% end %>
		</table>
		<% end %>
	
	</div>

</div>



